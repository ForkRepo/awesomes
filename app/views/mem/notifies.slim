=content_for :con
  ul.list-group.mem-list-wraper
    -@items.each do |item|
      li.list-group-item
        span =raw GitHub::Markdown.render item.desc
        a.right[href="#{item.link}"]: i.fa.fa-link
  div#page-mem      
  input#mem-nav[type="hidden" value="doc"]
  css:
    .list-group-item p{margin: 0; display: inline-block}
  javascript:
    $(function(){
      $("#page-mem").pagination(#{@count},{
        items_per_page : #{default_page_size},
        current_page : #{page},
        //link_to:"?page=__id__",
        callback: function(page){
          Turbolinks.visit("?page="+(page +1) );
        }
      }) 
    })
